<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="@color/LightOrange" >

<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/linearLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteX="16dp"
    tools:layout_editor_absoluteY="0dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/linearLayout"
        android:layout_width="413dp"
        android:layout_height="76dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/updateNoteText"
            android:layout_width="221dp"
            android:layout_height="84dp"
            android:layout_marginStart="26dp"
            android:layout_marginEnd="257dp"
            android:layout_marginBottom="12dp"
            android:fontFamily="@font/font2bold"
            android:paddingTop="50dp"
            android:text="@string/updateNote"
            android:textColor="@color/black"
            android:textSize="26sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/dateInputUpdate"
            android:layout_width="145dp"
            android:layout_height="48dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="12dp"
            android:autofillHints=""
            android:background="@color/LighterOrange"
            android:ems="10"
            android:fontFamily="@font/font2bold"
            android:hint="@string/createDate"
            android:inputType="date"
            android:textColorHint="#000000"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            tools:ignore="TouchTargetSizeCheck" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <EditText
        android:id="@+id/idInputUpdate"
        android:inputType="none"
        android:layout_width="371dp"
        android:layout_height="49dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="93dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="589dp"
        android:autofillHints=""
        android:background="@color/LighterOrange"
        android:ems="10"
        android:fontFamily="@font/font2bold"
        android:textColorHint="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/emotionInputUpdate"
        android:layout_width="371dp"
        android:layout_height="49dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="212dp"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="470dp"
        android:autofillHints=""
        android:background="@color/LighterOrange"
        android:ems="10"
        android:fontFamily="@font/font2bold"
        android:hint="@string/createEmotion"
        android:inputType="textPersonName"
        android:textColorHint="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/titleInputUpdate"
        android:layout_width="371dp"
        android:layout_height="49dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="270dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="412dp"
        android:autofillHints=""
        android:background="@color/LighterOrange"
        android:ems="10"
        android:fontFamily="@font/font2bold"
        android:hint="@string/createTitle"
        android:inputType="textPersonName"
        android:textColorHint="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/motivationalInputUpdate"
        android:layout_width="372dp"
        android:layout_height="49dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="152dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="530dp"
        android:autofillHints=""
        android:background="@color/LighterOrange"
        android:ems="10"
        android:fontFamily="@font/font2bold"
        android:hint="@string/createMotivationalMessage"
        android:inputType="textPersonName"
        android:textColorHint="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/textInputUpdate"
        android:layout_width="370dp"
        android:layout_height="302dp"
        android:layout_marginStart="23dp"
        android:layout_marginTop="328dp"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="101dp"
        android:autofillHints=""
        android:background="@color/LighterOrange"
        android:ems="10"
        android:fontFamily="@font/font2bold"
        android:gravity="top"
        android:hint="@string/createText"
        android:inputType="textPersonName"
        android:textColorHint="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/cancelButtonUpdate"
        android:layout_width="116dp"
        android:layout_height="48dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="647dp"
        android:layout_marginEnd="250dp"
        android:layout_marginBottom="36dp"
        android:background="@color/LighterOrange"
        android:fontFamily="@font/font2bold"
        android:text="@string/cancelOption"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/saveButtonUpdate"
        android:layout_width="116dp"
        android:layout_height="48dp"
        android:layout_marginStart="244dp"
        android:layout_marginTop="647dp"
        android:layout_marginEnd="51dp"
        android:layout_marginBottom="36dp"
        android:background="@color/LighterOrange"
        android:fontFamily="@font/font2bold"
        android:text="@string/saveOption"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>